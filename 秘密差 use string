#include <iostream>
#include <string>
using namespace std;

int main() {
  string xx;
  int odd=0, even=0;
  cin >> xx;
  for (int i = 0; i < xx.size(); i++){
    if (i % 2 == 0){
      odd = odd + xx[i] - '0';
    }
    else{
      even = even + xx[i] - '0';
    }
    }
  cout << abs(even - odd) << endl;
}
